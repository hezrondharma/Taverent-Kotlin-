<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SewaMain">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.906" />

    <LinearLayout
        android:layout_width="411dp"
        android:layout_height="60dp"
        android:gravity="center"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <LinearLayout
            android:id="@+id/btnHomeSewa"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imHomeSewa"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                app:srcCompat="@drawable/ic_baseline_home_24" />

            <TextView
                android:id="@+id/txHomeSewa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Home"
                android:textColor="#000000" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView4"
            android:layout_width="20dp"
            android:layout_height="45dp" />

        <LinearLayout
            android:id="@+id/btnChatSewa"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imChatSewa"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                app:srcCompat="@drawable/ic_baseline_chatgray_24" />

            <TextView
                android:id="@+id/txChatSewa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Chat"
                android:textColor="#C9C9C9" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView9"
            android:layout_width="20dp"
            android:layout_height="45dp" />

        <LinearLayout
            android:id="@+id/btnKelolaSewa"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imKelolaSewa"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                app:srcCompat="@drawable/ic_baseline_settingsgray_cell_24" />

            <TextView
                android:id="@+id/txKelolaSewa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Kelola"
                android:textColor="#C9C9C9" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView11"
            android:layout_width="20dp"
            android:layout_height="45dp" />

        <LinearLayout
            android:id="@+id/btnStatistikSewa"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imStatistikSewa"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                app:srcCompat="@drawable/ic_baseline_insert_chartgray_outlined_24" />

            <TextView
                android:id="@+id/txStatistikSewa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Statistik"
                android:textColor="#C9C9C9" />
        </LinearLayout>

        <TextView
            android:id="@+id/textView19"
            android:layout_width="20dp"
            android:layout_height="45dp" />

        <LinearLayout
            android:id="@+id/btnAkunSewa"
            android:layout_width="58dp"
            android:layout_height="55dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imAkunSewa"
                android:layout_width="match_parent"
                android:layout_height="38dp"
                app:srcCompat="@drawable/ic_baseline_persongray_24" />

            <TextView
                android:id="@+id/txAkunSewa"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Akun"
                android:textColor="#C9C9C9" />
        </LinearLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>